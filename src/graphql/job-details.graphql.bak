query GetCompleteJobDetails($jobId: ID!) {
  marketplaceJobPosting(id: $jobId) {
    # Main job fields
    id
    workFlowState {
      status
      closeResult
    }
    # canClientReceiveContractProposal  # requires view_client_company permission
    
    # Content fields
    content {
      title
      description
    }
    
    # Classification fields
    classification {
      category {
        id
        preferredLabel
        # parentCategory field not available in API
      }
      subCategory {
        id
        preferredLabel
        # parentCategory field not available in API
      }
      occupation {
        id
        preferredLabel
      }
      skills {
        id
        preferredLabel
        # ontologySkill field not available in API
      }
      additionalSkills {
        id
        preferredLabel
        # ontologySkill field not available in API
      }
    }
    
    # Contract Terms
    contractTerms {
      contractStartDate
      contractEndDate
      contractType
      onSiteType
      personsToHire
      experienceLevel
      notSurePersonsToHire
      notSureExperiencelevel
      
      # Fixed Price Contract Terms
      fixedPriceContractTerms {
        amount {
          rawValue
          currency
          displayValue
        }
        maxAmount {
          rawValue
          currency
          displayValue
        }
        engagementDuration {
          id
          label
          weeks
        }
      }
      
      # Hourly Contract Terms
      hourlyContractTerms {
        engagementDuration {
          id
          label
          weeks
        }
        engagementType
        notSureProjectDuration
        hourlyBudgetType
        hourlyBudgetMin
        hourlyBudgetMax
      }
    }
    
    # Contractor Selection
    contractorSelection {
      # Proposal Requirements
      proposalRequirement {
        coverLetterRequired
        freelancerMilestonesAllowed
        screeningQuestions {
          question
          sequenceNumber
        }
      }
      
      # Qualification Requirements
      # qualification {
        # Add qualification fields when available
        # id field not available in MarketplaceQualification
      # }
      
      # Location Requirements
      # location {
        # Add location fields when available
        # id field not available in MarketplaceLocation
      # }
    }
    
    # Ownership Information
    ownership {
      company {
        id
        name
        # description field not available in GenericOrganization
      }
      team {
        id
        name
        # description field not available in GenericOrganization
      }
    }
    
    # Public Company Information - requires view_client_company permission
    # clientCompanyPublic {
      # id
      # legacyType
      # teamsEnabled
      # canHire
      # hidden
      # country {
        # name
        # code field not available in Country
      # }
      # state
      # city
      # timezone
      # accountingEntity
      # billingType
      # paymentVerification {
        # status field requires OAuth permissions not available
        # verifiedAt field not available in PaymentVerificationResult
      # }
      # agencyDetails {
        # Add agency detail fields when available
        # id field not available in AgencyDetails
      # }
    # }
    
    # Activity Statistics - requires view_client_company permission
    # activityStat {
      # Applications and Bid Stats
      # applicationsBidStats {
        # avgRateBid {
          # rawValue
          # currency
          # displayValue
        # }
        # minRateBid {
          # rawValue
          # currency
          # displayValue
        # }
        # maxRateBid {
          # rawValue
          # currency
          # displayValue
        # }
        # avgInterviewedRateBid {
          # rawValue
          # currency
          # displayValue
        # }
      # }
      
      # Job Activity
      # jobActivity {
        # lastClientActivity  # requires view_client_company permission
        # invitesSent
        # totalInvitedToInterview
        # totalHired
        # totalUnansweredInvites
        # totalOffered
        # totalRecommended
      # }
    # }
    
    # Annotations
    annotations {
      tags
      customFields {
        key
        value
      }
    }
    
    # Attachments
    attachments {
      # Add attachment fields when available
      id
    }
    
    # Segmentation Data
    # segmentationData {
      # Add segmentation fields when available
      # id field not available in MarketplaceSegmentationData
    # }
    
    # Additional Search Info
    # additionalSearchInfo {
      # Add additional search info fields when available
      # id field not available in MarketplaceAdditionalSearchInfo
    # }
    
    # Client Proposals (pagination args not supported)
    # clientProposals {
      # Add client proposal fields when available
      # id field not available in ClientProposalsConnection
    # }
    
    # Custom Fields (pagination args not supported)
    # customFields {
      # Add custom field nodes when available
      # key and value fields not available in CustomFieldsConnection
    # }
  }
}